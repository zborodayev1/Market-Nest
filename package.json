    "react-toastify": "^11.0.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.3.0",
    "use-debounce": "^10.0.4",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
