{
  "name": "marketnest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build": "tsc -b && vite build --mode production",
    "deploy-fe": "set -e && rm -rf dist && echo -e \"\\033[1;31mDeleting dist...\\033[0m\" && echo -e \"\\033[1;34mBuilding...\\033[0m\" && npm run build && echo -e \"\\033[1;32mDeploying...\\033[0m\" && scp -r ./dist vps:/var/www/market-frontend && echo -e \"\\033[1;33mRestarting Nginx...\\033[0m\" && ssh vps 'sudo systemctl restart nginx' && echo -e \"\\033[1;32mSuccess!\\033[0m\"",
    "deploy-be": "echo -e \"\\033[1;34mDeploying backend...\\033[0m\" && scp -r ./server vps:~/market-nest && scp ./package.json vps:~/market-nest && scp ./index.js vps:~/market-nest && scp ./.env.production vps:~/market-nest/.env && echo -e \"\\033[1;32mBackend deployment completed!\\033[0m\"",
    "deploy-all": "npm run deploy-fe && npm run deploy-be",
    "lint": "eslint .",
    "preview": "vite preview --mode production",
    "node": "nodemon index.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.1",
    "@mui/icons-material": "^6.4.11",
    "@mui/material": "^6.4.11",
    "@reduxjs/toolkit": "^2.3.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.3",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "framer-motion": "^11.11.11",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.460.0",
    "mongoose": "^8.7.2",
    "motion": "^11.11.17",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.16",
    "nodemon": "^3.1.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
